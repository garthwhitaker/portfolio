{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "This is my portfolio project ",
  "main": "index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "test": "jest",
    "build": "webpack -d",
    "build:production": "webpack -p --config ./webpack.prod.js",
    "build:watch": "webpack -d --watch",
    "start": "webpack-dev-server --config ./webpack.dev.js",
    "start:dashboard": "webpack-dashboard -- webpack-dev-server --config ./webpack.dev.js",
    "eslint": "eslint client/app",
    "eslint:fix": "eslint --fix .",
    "test:bundlesize": "bundlesize",
    "docker:build": "docker build -t portfolio .",
    "docker:deploy": "docker run --name portfolio-instance -p 80:80 -d portfolio",
    "docker:destroy": "docker stop portfolio-instance && docker rm portfolio-instance && docker rmi portfolio",
    "start:lighthouse:local": "lighthouse http://localhost:80 --view"
  },
  "author": "Garth Whitaker",
  "license": "ISC",
  "dependencies": {
    "babel-jest": "^22.4.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.18",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-plugin-jest": "^21.14.1",
    "html-webpack-plugin": "^3.0.4",
    "lighthouse": "^2.9.3",
    "material-ui": "^1.0.0-beta.34",
    "material-ui-icons": "^1.0.0-beta.17",
    "npm": "^5.7.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dashboard": "^1.1.1",
    "webpack-merge": "^4.1.2"
  },
  "private": true,
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "bundlesize": "^0.16.0",
    "css-loader": "^0.28.9",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.9",
    "image-webpack-loader": "^4.1.0",
    "jest": "^22.4.2",
    "react-test-renderer": "^16.2.0",
    "style-loader": "^0.20.2",
    "url-loader": "^0.6.2",
    "webpack-dev-server": "^2.11.1"
  },
  "bundlesize": [
    {
      "path": "./dist/vendor.*.js",
      "maxSize": "78 kB"
    },
    {
      "path": "./dist/main.*.js",
      "maxSize": "8.1 kB"
    },
    {
      "path": "./dist/index.html",
      "maxSize": "1 kB"
    }
  ],
  "jest": {
    "setupFiles": [
      "./setupFile.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
