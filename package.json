{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "This is my portfolio project ",
  "main": "index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "test": "jest",
    "build": "webpack --config ./webpack.dev.js",
    "build:production": "webpack -p --config ./webpack.prod.js",
    "build:watch": "webpack -d --watch",
    "start": "webpack-dev-server --config ./webpack.dev.js",
    "start:dashboard": "webpack-dashboard -- webpack-dev-server --config ./webpack.dev.js",
    "eslint": "eslint client/app",
    "eslint:fix": "eslint --fix .",
    "test:bundlesize": "bundlesize",
    "docker:build": "docker build -t portfolio .",
    "docker:deploy": "docker run --name portfolio-instance -p 80:80 -d portfolio",
    "docker:destroy": "docker stop portfolio-instance && docker rm portfolio-instance && docker rmi portfolio",
    "start:lighthouse:local": "lighthouse http://localhost:80 --view"
  },
  "author": "Garth Whitaker",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@loadable/component": "5.10.3",
    "@material-ui/core": "4.6.0",
    "@material-ui/icons": "4.5.1",
    "@material-ui/styles": "4.10.0",
    "@types/react": "16.9.52",
    "@types/react-dom": "16.9.8",
    "babel-jest": "25.0.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.18",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "eslint-plugin-jest": "23.0.3",
    "html-webpack-plugin": "3.2.0",
    "lighthouse": "6.4.1",
    "prop-types": "15.7.2",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-router-dom": "5.1.2",
    "webpack": "4.44.2",
    "webpack-bundle-analyzer": "3.9.0",
    "webpack-dashboard": "2.1.0",
    "webpack-merge": "4.2.2"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "7.7.2",
    "@babel/preset-env": "7.7.1",
    "@babel/preset-react": "7.7.0",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.6",
    "babel-preset-es2015": "^6.24.1",
    "bundlesize": "0.18.0",
    "css-loader": "3.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.16.0",
    "file-loader": "4.2.0",
    "image-webpack-loader": "6.0.0",
    "jest": "25.0.0",
    "prettier": "2.1.2",
    "react-test-renderer": "16.11.0",
    "source-map-loader": "1.1.1",
    "style-loader": "1.0.0",
    "ts-loader": "8.0.5",
    "typescript": "4.0.3",
    "url-loader": "2.2.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.11.0"
  },
  "bundlesize": [
    {
      "path": "./dist/vendor.*.js",
      "maxSize": "78 kB"
    },
    {
      "path": "./dist/main.*.js",
      "maxSize": "8.1 kB"
    },
    {
      "path": "./dist/index.html",
      "maxSize": "1 kB"
    }
  ],
  "jest": {
    "setupFiles": [
      "./setupFile.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
